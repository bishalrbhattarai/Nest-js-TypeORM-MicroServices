generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(cuid())
  name      String?
  email     String
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  // Bookmark  Bookmark[]
}

model Bookmark {
  id          String   @id @default(cuid())
  title       String
  description String?
  link        String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  // user      User     @relation(fields: [userId], references: [id])
  // userId    String
}
